@model List<Student>
@{
    ViewData["Title"] = "Index";
}

<h1>Index</h1>
<table class="table table-bordered">
    @foreach (var item in Model)
    {
        <tr>
            <td>@item.Name</td>
            <td>@item.Department.Name</td>
            <td><a href="/Student/Edit/@item.Id">Edit</a></td>
        </tr>
    }
</table>

